{
  "name": "openrest4js",
  "version": "1.0.2",
  "description": "The Openrest SDK for JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --require test/setup.js \"test/**/*.test.js\"",
    "lint": "eslint ./src ./test",
    "posttest": "npm run lint",
    "compile": "babel -d dist/ src/",
    "prepublish": "npm run snyk-protect && npm test && npm run compile",
    "build": ":",
    "release": "npm install wnpm-ci --no-save && wnpm-release -- --no-shrinkwrap",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/openrest4js.git"
  },
  "keywords": [
    "openrest"
  ],
  "author": "Wix Restaurants",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wix/openrest4js/issues"
  },
  "homepage": "https://github.com/wix/openrest4js",
  "dependencies": {
    "availability": "^2.1.254",
    "lodash": "^4.8.2",
    "moment": "^2.15.2",
    "moment-timezone": "^0.5.7",
    "wix-restaurants-js-sdk": "^1.0.197",
    "snyk": "^1.317.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^3.9.0",
    "http": "0.0.0",
    "mocha": "^2.4.5",
    "nock": "^9.0.2",
    "xhr2": "^0.1.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "directories": {
    "test": "test"
  },
  "snyk": true
}
